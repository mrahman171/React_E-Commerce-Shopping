st Products = () => {\n  const [data, setData] = useState([]);\n  const [filter, setFilter] = useState(data);\n  const [loading, setLoading] = useState(false);\n  let componentMounted = true;\n\n  const dispatch = useDispatch();\n\n  const addProduct = (product) => {\n    dispatch(addCart(product))\n  }\n\n  useEffect(() => {\n    const getProducts = async () => {\n      setLoading(true);\n      const response = await fetch(\"./assets/information.json\");\n      if (componentMounted) {\n        setData(await response.clone().json());\n        setFilter(await response.json());\n        setLoading(false);\n      }\n\n      return () => {\n        componentMounted = false;\n      };\n    };\n\n    getProducts();\n  }, []);\n\n  const Loading = () => {\n    return (\n      <>\n        <div className=\"col-12 py-5 text-center\">\n          <Skeleton height={40} width={560} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n        <div className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n          <Skeleton height={592} />\n        </div>\n      </>\n    );\n  };\n\n  const filterProduct = (cat) => {\n    const updatedList = data.filter((item) => item.category === cat);\n    setFilter(updatedList);\n  }\n  const ShowProducts = () => {\n    return (\n      <>\n        <div className=\"buttons text-center py-5\">\n          <button className=\"btn btn-outline-dark btn-sm m-2\" onClick={() => setFilter(data)}>All</button>\n          <button className=\"btn btn-outline-dark btn-sm m-2\" onClick={() => filterProduct(\"men's clothing\")}>Men's Clothing</button>\n          <button className=\"btn btn-outline-dark btn-sm m-2\" onClick={() => filterProduct(\"women's clothing\")}>\n            Women's Clothing\n          </button>\n          <button className=\"btn btn-outline-dark btn-sm m-2\" onClick={() => filterProduct(\"jewelery\")}>Jewelery</button>\n          <button className=\"btn btn-outline-dark btn-sm m-2\" onClick={() => filterProduct(\"electronics\")}>Electronics</button>\n        </div>\n\n        {filter.map((product) => {\n          return (\n            <div id={product.id} key={product.id} className=\"col-md-4 col-sm-6 col-xs-8 col-12 mb-4\">\n              <div className=\"card text-center h-100\" key={product.id}>\n                <img\n                  className=\"card-img-top p-3\"\n                  src={product.image}\n                  alt=\"Card\"\n                  height={300}\n                />\n                <div className=\"card-body\">\n                  <h5 className=\"card-title\">\n                    {product.title.substring(0, 12)}...\n                  </h5>\n                  <p className=\"card-text\">\n                    {product.description.substring(0, 90)}...\n                  </p>\n                </div>\n                <ul className=\"list-group list-group-flush\">\n                  <li className=\"list-group-item lead\">BDT {product.price}</li>\n                  {/* <li className=\"list-group-item\">Dapibus ac facilisis in</li>\n                    <li className=\"list-group-item\">Vestibulum at eros</li> */}\n                </ul>\n                <div className=\"card-body\">\n                  <Link to={\"/product/\" + product.id} className=\"btn btn-dark m-1\">\n                    Buy Now\n                  </Link>\n                  <button className=\"btn btn-dark m-1\" onClick={() => addProduct(product)}>\n                    Add to Cart\n                  </button>\n                </div>\n              </div>\n            </div>\n\n          );\n        })}\n      </>\n    );\n  };\n  return (\n    <>\n      <div className=\"container my-3 py-3\">\n        <div className=\"row\">\n          <div className=\"col-12\">\n            <h2 className=\"display-5 text-center\">Latest Products</h2>\n            <hr />\n          </div>\n        </div>\n        <div className=\"row justify-content-center\">\n          {loading ? <Loading /> : <ShowProducts />}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Products;\n","C:\\Users\\HP\\Downloads\\New folder\\React_E-Commerce-main\\src\\components\\Footer.jsx",[],[],{"ruleId":"113","severity":1,"message":"114","line":16,"column":13,"nodeType":"115","messageId":"116","endLine":16,"endColumn":19},{"ruleId":"113","severity":1,"message":"114","line":25,"column":13,"nodeType":"115","messageId":"116","endLine":25,"endColumn":19},{"ruleId":"113","severity":1,"message":"114","line":29,"column":13,"nodeType":"115","messageId":"116","endLine":29,"endColumn":19},{"ruleId":"117","severity":1,"message":"118","line":33,"column":28,"nodeType":"119","endLine":33,"endColumn":33},"no-unreachable","Unreachable code.","BreakStatement","unreachableCode","react-hooks/exhaustive-deps","Assignments to the 'componentMounted' variable from inside React Hook useEffect will be lost after each render. To preserve the value over time, store it in a useRef Hook and keep the mutable value in the '.current' property. Otherwise, you can move this variable directly inside useEffect.","Literal"]